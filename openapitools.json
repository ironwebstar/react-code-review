{
  "$schema": "node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "5.3.0",
    "generators": {
      "api": {
        "glob": "./ckw/models/src/main/resources/api.yml",
        "generatorName": "typescript-axios",
        "output": "src/data/generated-sources/openapi",
        "additionalProperties": {
          "enumPropertyNaming": "original",
          "enumNameSuffix": "enum",
          "supportsES6": true,
          "withInterfaces": true
        }
      }
    }
  }
}